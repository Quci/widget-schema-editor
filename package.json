  "license": "MIT",
  "main": "dist/index.umd.js",
  "scripts": {
    "dev": "akfun dev",
    "build2lib": "akfun build2lib",
    "build2demo": "akfun build",
    "format": "prettier --write \"src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\""
  },
  "files": [
    "dist/*"
  ],
  "husky": {
