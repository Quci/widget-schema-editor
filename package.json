    "@wibetter/ui-schema-editor": "^1.1.0",
    "ace-builds": "^1.4.12",
    "antd": "4.7.3",
    "mobx": "5.9.4",
    "mobx-react": "6.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-ace": "^9.4.0",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "akfun": "^1.5.12",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1"
  },
  "browserslist": [
    "> 1%",
